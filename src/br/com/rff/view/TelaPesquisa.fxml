<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="pnPesquisa" prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="br.com.rff.controller.TelaPesquisaController">
  <center>
    <TableView fx:id="tblDados" prefHeight="200.0" prefWidth="200.0" />
  </center>
  <top>
    <HBox alignment="CENTER_LEFT" prefHeight="60.0" prefWidth="200.0" spacing="10.0">
      <children>
        <TextField id="labelpesquisar" fx:id="txtPesquisa" prefWidth="344.0" promptText="Pesquisar o que procura" />
        <Button id="btnpesquisar" fx:id="btnPesquisar" mnemonicParsing="false" onAction="#pesquisarClicked" text="Pesquisar" />
      </children>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </HBox>
  </top>
</BorderPane>
